{
  "name": "polling-booth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "migrate:up": "node ./node_modules/db-migrate/bin/db-migrate up",
    "migrate:down": "node ./node_modules/db-migrate/bin/db-migrate down",
    "migrate:create": "node ./node_modules/db-migrate/bin/db-migrate create",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run migrate:up"
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.3",
    "bull": "^3.5.2",
    "cookie-parser": "~1.4.3",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.1",
    "morgan": "~1.9.0",
    "nodemailer": "^5.1.1",
    "orm": "^5.0.2",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-egg": "^7.1.0",
    "husky": "^1.2.1"
  }
}
